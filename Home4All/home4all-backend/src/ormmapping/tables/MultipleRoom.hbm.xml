<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="business.entities.MultipleRoom" table="MultipleRoom" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="peopleAmount" column="PeopleAmount" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="quantity" column="Quantity" type="integer" length="10" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
